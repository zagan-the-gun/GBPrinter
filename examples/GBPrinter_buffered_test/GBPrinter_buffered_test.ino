  uint8_t buffer[640];

void setup() {
  setupPrinter(GBIn, GBOut, GBClock);
  Serial.begin(9600);
  Serial.println("Gameboy Printer for Arduino"); 
  Serial.println("Enter 'h' to print array"); 
}

void loop() {
  if (Serial.available() > 0) {
    cmd = Serial.read();
    if(cmd == 'h') {
      hadcon();
    }
  }
}

void sendRow (uint8_t row2send[]) {
    CRC = 0;
    Serial.println("Data");
    CRC += beginData();
    for(int i=0; i<640; ++i) {
      uint8_t cmd = row2send[i];
      CRC += cmd;
      GBSerialOut(cmd);
    }
    if(endData(CRC)) {
      Serial.println("Data sent");
    }
    printStatusCode(-1);
}

  void hadcon() {
    
    for (int j=0;j<23;j++) {
      
      for (int i=0;i<640;i++) {
        buffer[i] = pgm_read_byte_near(pgm_read_word(row_table +j) + i);
      }
      
      sendInitialize();
      
      getStatusCode();

      sendRow(buffer);    
      
      sendInquiry();
      GBPCommand(GBData,0);
      printStatusCode(-1);

      
      uint8_t margin_left = 1;
      uint8_t margin_right = 3;
      if (j == 0) {
        sendPrint(margin_left,0,0xE4,0x40);
      }
//      else if (j == 16) {
//        sendPrint(0,margin_right,0xE4,0x40);
//      }
      else {
        sendPrint(0,0,0xE4,0x40);
      }

      // code from furrtek, wait for line to finish and print on
      while(1) {
         uint8_t inq = sendInquiry();
         if (inq & 2) {
           break;
         }
         delay(100);
         // printing started
      }
      while(1) {
         uint8_t inq = sendInquiry();
         if (!(inq & 2)) {
           break;
         }
         delay(100);
      }
    }
    sendPrint(0,30,0xE4,0x40);
  }
